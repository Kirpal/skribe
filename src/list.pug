doctype html
html
    head
        link(rel="stylesheet", href="/static/list.css", type="text/css")

    body
        include header.pug
        section#live
        each val in [1, 2, 3, 5, 6, 7, 8, 9]
            div.live-preview
                div.words
                    div.old The Man Jumped off the building and his name was
                    div.new Joe
                div.overlay
                    div.titles
                        div.title Story #{val}
                        div.genre Fantasy
                    div.viewers
                        div.icon
                        div.number 156